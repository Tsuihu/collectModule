<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.higher.scancodescollect.dao.ManagerDao">
    <select id="getManagerByTel" resultType="com.higher.scancodescollect.pojo.Manager">
        select *
        from manager
        where tel = #{tel};
    </select>
</mapper>